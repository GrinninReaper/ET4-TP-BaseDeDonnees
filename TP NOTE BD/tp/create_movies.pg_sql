CREATE TABLE PEOPLE (pid INTEGER PRIMARY KEY, firstname VARCHAR(30), lastname VARCHAR(30));
CREATE TABLE MOVIE (mid INTEGER PRIMARY KEY, title VARCHAR(90) NOT NULL, year INTEGER NOT NULL, runtime INTEGER NOT NULL, rank INTEGER NOT NULL);

CREATE TABLE ROLE (mid INTEGER REFERENCES MOVIE, pid INTEGER REFERENCES PEOPLE, name VARCHAR(70));
CREATE TABLE DIRECTOR (mid INTEGER REFERENCES MOVIE, pid INTEGER REFERENCES PEOPLE,  CONSTRAINT dir_const UNIQUE(mid,pid));


